{
  "manifest_version": 2,
  "name": "4ksight",
  "version": "1.0",
  "description": "Display review score on reviews page.",
  "content_scripts": [
    {
      "matches": [
        "https://pitchfork.com/reviews/*",
        "https://pitchfork.com/best/*"
      ],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
